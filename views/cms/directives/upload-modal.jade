.upload-modal(
  style='position: fixed; top: 50%; right: 50%; background-color: white; z-index: 91;')
  h2 Новая папка
  input(type='text' ng-model='directoryName')
  button(ng-click='addDirectory()') Добавить
  button(ng-click='dismiss()') Отмена
.splash(ng-click='dismiss()'
  style='position: fixed; left: 0%; top: 0%; width: 100%; height: 100%; z-index: 90;')
